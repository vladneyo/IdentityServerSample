@model List<IdentityServerSample.IdentityServer.AdminPanel.Data.Dtos.UserDto>

@{
    ViewData["Title"] = "Users";
}

<h2>Users</h2>

<ul>
    @foreach (var user in Model)
    {
        <li>@user.Id - @user.UserName
            <ul>
                @foreach(var claim in user.Claims)
                {
                    <li>@claim.Value.Type - @claim.Value.Value</li>
                }
            </ul>
            <a asp-action="Edit" asp-controller="Users" asp-route-id="@user.Id">Edit</a>
        </li>
    }
</ul>


